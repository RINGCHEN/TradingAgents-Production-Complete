            "format": "table",
            "rawSql": "SELECT\n  COUNT(*) AS \"總評估數\",\n  COUNT(CASE WHEN evaluation_time > NOW() - INTERVAL '1 hour' THEN 1 END) AS \"過去1小時\",\n  COUNT(CASE WHEN evaluation_time > NOW() - INTERVAL '1 day' THEN 1 END) AS \"過去24小時\"\nFROM\n  ai_quality_evaluations",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
