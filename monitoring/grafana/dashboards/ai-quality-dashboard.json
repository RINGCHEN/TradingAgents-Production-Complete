    "panels": [
      {
        "id": 1,
        "title": "AI 品質分數趨勢",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "format": "time_series",
            "rawSql": "SELECT\n  $__timeGroupAlias(evaluation_time, '1h') as time,\n  AVG(relevance) AS \"平均相關性\",\n  AVG(coherence) AS \"平均連貫性\",\n  AVG(insightfulness) AS \"平均洞察力\",\n  AVG(task_completion) AS \"平均任務完成度\"\nFROM\n  ai_quality_evaluations\nWHERE\n  $__timeFilter(evaluation_time)\nGROUP BY time\nORDER BY time",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "none",
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "pointSize": 5,
              "scaleDistribution": {
                "type": "linear"
              },
              "showPoints": "auto",
              "spanNulls": false,
              "stacking": {
                "group": "A",
                "mode": "none"
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "min": 0,
            "max": 5,
            "unit": "short"
          }
        },
        "options": {
          "legend": {
            "calcs": [],
            "displayMode": "list",
            "placement": "bottom"
          },
          "tooltip": {
            "mode": "single",
            "sort": "none"
          }
        }
      },
      {
        "id": 2,
        "title": "即時品質分佈",
        "type": "stat",
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 0
        },
        "targets": [
          {
            "format": "table",
            "rawSql": "SELECT\n  AVG(relevance) AS \"相關性\",\n  AVG(coherence) AS \"連貫性\",\n  AVG(insightfulness) AS \"洞察力\",\n  AVG(task_completion) AS \"任務完成度\"\nFROM\n  ai_quality_evaluations\nWHERE\n  evaluation_time > NOW() - INTERVAL '1 hour'",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
