        "targets": [
          {
            "format": "table",
            "rawSql": "SELECT\n  evaluation_time AS \"評估時間\",\n  LEFT(prompt, 100) || CASE WHEN LENGTH(prompt) > 100 THEN '...' ELSE '' END AS \"提示 (前100字)\",\n  LEFT(response, 150) || CASE WHEN LENGTH(response) > 150 THEN '...' ELSE '' END AS \"回應 (前150字)\",\n  relevance AS \"相關性\",\n  coherence AS \"連貫性\",\n  insightfulness AS \"洞察力\",\n  task_completion AS \"任務完成\",\n  LEFT(justification, 200) || CASE WHEN LENGTH(justification) > 200 THEN '...' ELSE '' END AS \"評估理由\"\nFROM\n  ai_quality_evaluations\nWHERE\n  $__timeFilter(evaluation_time)\nORDER BY evaluation_time DESC\nLIMIT 50",
