  "shortcuts": [
    {
      "name": "股票搜尋",
      "short_name": "搜尋",
      "description": "快速搜尋台股資訊",
      "url": "/search",
      "icons": [
        {
          "src": "/assets/icons/search-96x96.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "對話分析",
      "short_name": "分析",
      "description": "AI對話式股票分析",
      "url": "/dialogue",
      "icons": [
        {
          "src": "/assets/icons/chat-96x96.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "管理後台",
      "short_name": "管理",
      "description": "系統管理介面",
      "url": "/admin",
      "icons": [
        {
          "src": "/assets/icons/admin-96x96.png",
          "sizes": "96x96"
        }
      ]
    }
  ],
  
  "categories": [
    "finance",
    "business",
    "productivity",
    "utilities"
  ],
  
  "screenshots": [
    {
      "src": "/assets/screenshots/desktop-1280x720.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide",
      "label": "TradingAgents 桌面版主介面"
    },
    {
      "src": "/assets/screenshots/mobile-390x844.png",
      "sizes": "390x844",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "TradingAgents 手機版介面"
    }
  ],
  
  "related_applications": [],
  "prefer_related_applications": false,
  
  "serviceworker": {
    "src": "/sw.js",
    "scope": "/",
    "update_via_cache": "imports"
  },
  
  "protocol_handlers": [
    {
      "protocol": "web+tradingagents",
      "url": "/?stock=%s"
    }
  ],
  
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "file",
          "accept": ["image/*", ".csv", ".xlsx"]
        }
      ]
    }
  },
  
  "file_handlers": [
    {
      "action": "/file-handler",
      "accept": {
        "text/csv": [".csv"],
        "application/vnd.openxmlformats-officedocument.spreadsheetml.sheet": [".xlsx"]
      }
    }
  ],
  
  "edge_side_panel": {
    "preferred_width": 400
  },
  
  "launch_handler": {
    "client_mode": ["navigate-existing", "navigate-new"]
  }
}